@page
@model RegisterModel
@{
    ViewData["Title"] = "Kayıt";
}

<!DOCTYPE html>
<html lang="en">
<head>
	<title>Kayıt</title>
	<meta charset="UTF-8">
	<meta name="viewport" content="width=device-width, initial-scale=1">
	<!--===============================================================================================-->
	<!--===============================================================================================-->
	<link rel="stylesheet" type="text/css" href="~/vendor/bootstrap/css/bootstrap.min.css">
	<!--===============================================================================================-->
	<link rel="stylesheet" type="text/css" href="~/fonts/font-awesome-4.7.0/css/font-awesome.min.css">
	<!--===============================================================================================-->
	<link rel="stylesheet" type="text/css" href="~/fonts/iconic/css/material-design-iconic-font.min.css">
	<!--===============================================================================================-->
	<link rel="stylesheet" type="text/css" href="~/vendor/animate/animate.css">
	<!--===============================================================================================-->
	<link rel="stylesheet" type="text/css" href="~/vendor/css-hamburgers/hamburgers.min.css">
	<!--===============================================================================================-->
	<link rel="stylesheet" type="text/css" href="~/vendor/animsition/css/animsition.min.css">
	<!--===============================================================================================-->
	<link rel="stylesheet" type="text/css" href="~/vendor/select2/select2.min.css">
	<!--===============================================================================================-->
	<link rel="stylesheet" type="text/css" href="~/vendor/daterangepicker/daterangepicker.css">
	<!--===============================================================================================-->
	<link rel="stylesheet" type="text/css" href="~/css/util.css">
	<link href="~/css/main.css" rel="stylesheet" />
	<link href="~/css/falan.css" rel="stylesheet" />

	<!--===============================================================================================-->
</head>
<body>

	<div class="limiter">
		<div class="container-login100">
			<div class="wrap-login100">


				<form class="login100-form validate-form " id="registerForm" asp-route-returnUrl="@Model.ReturnUrl" method="post">
						<span class="login100-form-title p-b-48">
						</span>
						<span class="login100-form-title p-b-26">
							Hoşgeldiniz

						</span>
						<span class="login100-form-title p-b-48">
						<h4>Yeni Hesap Oluşturunuz</h4>
						</span>
						<div asp-validation-summary="ModelOnly" class="text-danger" role="alert"></div>

						<div class="wrap-input100 validate-input" data-validate="Valid email is: a@b.c">
							<input asp-for="Input.Email" class="form-control input100" autocomplete="username" aria-required="true" placeholder="Eposta" />
							<span asp-validation-for="Input.Email" class="text-danger"></span>
						</div>

						<div class="wrap-input100 validate-input" data-validate="Şifrenizi Giriniz">
							<span class="btn-show-pass">
								<i class="zmdi zmdi-eye"></i>
							</span>
							<input asp-for="Input.Password" class="form-control input100" autocomplete="current-password" aria-required="true" placeholder="Şifre" />
							<span asp-validation-for="Input.Password" class="text-danger"></span>
						</div>
						<div class="wrap-input100 validate-input" data-validate="Şifrenizi Giriniz">
							<span class="btn-show-pass">
								<i class="zmdi zmdi-eye"></i>
							</span>
							<input asp-for="Input.ConfirmPassword" class="form-control input100" autocomplete="new-password" aria-required="true" placeholder="Şifre Tekrar" />
							<span asp-validation-for="Input.ConfirmPassword" class="text-danger"></span>
						</div>

						<div class="container-login100-form-btn">
							<div class="wrap-login100-form-btn">
								<div class="login100-form-bgbtn"></div>
								<div>
										<button id="registerSubmit" type="submit" class="login100-form-btn">Kayıt</button>
								</div>
							</div>
						</div>
					<div class="text-center p-t-25	">
		
					</div>
					</form>
			</div>

		</div>
	</div>
	@section Scripts {
		<partial name="_ValidationScriptsPartial" />
	}

	<div id="dropDownSelect1"></div>

	<!--===============================================================================================-->
	<script src="~/vendor/jquery/jquery-3.2.1.min.js"></script>
	<!--===============================================================================================-->
	<script src="~/vendor/animsition/js/animsition.min.js"></script>
	<!--===============================================================================================-->
	<script src="~/vendor/bootstrap/js/popper.js"></script>
	<script src="~/vendor/bootstrap/js/bootstrap.min.js"></script>
	<!--===============================================================================================-->
	<script src="~/vendor/select2/select2.min.js"></script>
	<!--===============================================================================================-->
	<script src="~/vendor/daterangepicker/moment.min.js"></script>
	<script src="~/vendor/daterangepicker/daterangepicker.js"></script>
	<!--===============================================================================================-->
	<script src="~/vendor/countdowntime/countdowntime.js"></script>
	<!--===============================================================================================-->
	<script src="~/js/main.js"></script>

</body>
</html>

